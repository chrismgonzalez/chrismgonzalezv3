{
  "name": "chrismgonzalezv3",
  "description": "v3 of my personal site, chrismgonzalez.com",
  "version": "3.0.0",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "check": "npx npm-check-updates -u"
  },
  "dependencies": {


    "gatsby": "^2.17.4",
    "gatsby-plugin-catch-links": "^2.1.12",
    "gatsby-image": "^2.2.24",  
    "gatsby-plugin-feed": "^2.3.15",
    "gatsby-plugin-google-analytics": "^2.1.23",
    "gatsby-plugin-manifest": "^2.2.20",
    "gatsby-plugin-netlify": "^2.1.22",
    "gatsby-plugin-nprogress": "^2.1.9",
    "gatsby-plugin-offline": "^3.0.16",
    "gatsby-plugin-sass": "^2.1.17",
    "gatsby-plugin-react-helmet": "3.1.18",
    "gatsby-plugin-sharp": "^2.2.28",

    "gatsby-plugin-sitemap": "2.2.24",
    "gatsby-plugin-twitter": "^2.1.9",
    "gatsby-remark-autolink-headers": "^2.1.13",
    "gatsby-plugin-typography": "^2.3.22",
