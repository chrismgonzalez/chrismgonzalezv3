    "gatsby-remark-images": "3.1.25",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-remark-prismjs": "3.3.31",
    "gatsby-transformer-sharp": "2.3.9",
    "gatsby-transformer-remark": "2.6.27",
    "lodash": "^4.17.13",
    "lodash.kebabcase": "^4.1.1",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "prismjs": "^1.19.0",
    "react": "^16.10.2",
    "react-dom": "^16.11.0",
    "react-github-btn": "^1.0.6",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.19",
    "sharp": "^0.23.1",
    "typography": "^0.16.19",
    "url-join": "^4.0.0"
